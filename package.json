{
  "name": "sweepsteak-backend",
  "version": "1.0.82",
  "description": "New backend project starter",
  "main": "build/index.js",
  "private": "true",
  "scripts": {
    "local": "cross-env APPLIED_ENV=local nodemon --config \"./nodemon.json\"/",
    "local:noauth": "cross-env APPLIED_ENV=local NO_AUTH=1 nodemon --config \"./nodemon.json\"/",
    "start:local": "cross-env APPLIED_ENV=local node build/index.js",
    "build:local": "cross-env APPLIED_ENV=local tsc",
    "dev": "cross-env APPLIED_ENV=development nodemon --config \"./nodemon.json\"/",
    "dev:noauth": "cross-env APPLIED_ENV=development NO_AUTH=1 nodemon --config \"./nodemon.json\"/",
    "start:dev": "cross-env APPLIED_ENV=development node build/index.js",
    "build:dev": "cross-env APPLIED_ENV=development tsc",
    "prod": "cross-env APPLIED_ENV=production nodemon --config \"./nodemon.json\"/",
    "prod:noauth": "cross-env APPLIED_ENV=production NO_AUTH=1 nodemon --config \"./nodemon.json\"/",
    "start:prod": "cross-env APPLIED_ENV=production node build/src/index.js",
    "build:prod": "cross-env APPLIED_ENV=production tsc",
    "start": "tsc && node build/src/index.js",
    "test": "yarn jest",
    "update:players": "ts-node src/scripts/updatePlayerScores.ts",
    "ts-check": "yarn tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"*.{ts,js,json}\"",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "prepare": "husky"
  },
  "author": "koppany kondricz",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/cloud-sql-connector": "^1.8.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "axios": "1.10.0",
    "cors": "2.8.5",
    "dayjs": "1.11.13",
    "dotenv": "16.4.5",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "0.42.0",
    "express": "5.1.0",
    "express-validator": "7.2.1",
    "firebase": "^12.3.0",
    "firebase-admin": "13.4.0",
    "nanoid": "^5.1.6",
    "pg": "^8.16.3",
    "postgres": "3.4.5",
    "ts-node": "10.9.2",
    "twilio": "^5.10.5"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@wesleytodd/openapi": "^1.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "glob": "^10.3.10",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
